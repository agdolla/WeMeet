/**
 * If shouldHide is true, returns a CSS class that hides the element.
 */
function hideElement(shouldHide) {
  if (shouldHide) {
    return 'hidden';
  } else {
    return '';
  }
}
function disabledElement(shouldHide) {
  if (shouldHide) {
    return 'disabled';
  } else {
    return '';
  }
}